def add(a, b):
    return a + b

def main():
    x = [1, 2, 3]
    print("Array:", x)
    x[1] = 10
    print("Modified:", x)
    print("Index 1:", x[1])
    
    res = add(5, 5)
    print("Sum 5+5:", res)
    
    name = "World"
    print(f"Hello, {name}!")
    
    try:
        print("In try")
        raise "Ouch"
    except:
        print("Caught exception")

